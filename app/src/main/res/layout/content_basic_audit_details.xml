<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="10dp"
    android:paddingLeft="10dp"
    android:paddingRight="10dp"
    android:paddingTop="10dp"
    android:background="#1A000000"
    android:focusable="true"
    android:focusableInTouchMode="true"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="cied.in.Utz.BasicAuditDetailsActivity"
    tools:showIn="@layout/activity_basic_audit_details">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <android.support.v7.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/topLayout"
                app:cardCornerRadius="6dp"
                android:layout_marginBottom="5dp">
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@drawable/spinner_bg"
                    >

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="N"
                        android:textColor="@color/colorPrimaryDark"
                        android:id="@+id/textView15"
                        android:layout_alignParentTop="true"
                        android:layout_centerHorizontal="true" />

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="55dp"
                        android:layout_marginTop="15dp"
                        android:layout_marginLeft="10dp"
                        android:layout_alignParentTop="true"
                        android:background="@drawable/yellow_btn_bg"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:layout_toLeftOf="@+id/textView15"
                        android:layout_toStartOf="@+id/textView15"
                        android:id="@+id/relativeLayout9">

                        <ImageView
                            android:layout_width="25dp"
                            android:layout_height="25dp"
                            android:id="@+id/imageView8"
                            android:src="@drawable/sync_button"
                            android:layout_marginLeft="15dp"
                            android:layout_centerVertical="true"
                            android:layout_alignParentLeft="true"
                            android:layout_alignParentStart="true" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="SYNC"
                            android:textSize="18sp"
                            android:layout_marginLeft="10dp"
                            android:textStyle="bold"
                            android:textColor="@color/dark_yellow"
                            android:id="@+id/syncBtn"
                            android:layout_centerVertical="true"
                            android:layout_centerHorizontal="true" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_alignTop="@+id/relativeLayout9"
                        android:background="@drawable/yellow_btn_bg"
                        android:layout_toRightOf="@+id/textView15"
                        android:layout_marginRight="10dp"
                        android:layout_toEndOf="@+id/textView15"
                        android:layout_alignBottom="@+id/relativeLayout9"
                        android:id="@+id/relativeLayout11">

                        <ImageView
                            android:layout_width="25dp"
                            android:layout_height="25dp"
                            android:id="@+id/imageView9"
                            android:src="@drawable/submit_button"
                            android:layout_marginLeft="5dp"
                            android:layout_centerVertical="true"
                            android:layout_alignParentLeft="true"
                            android:layout_alignParentStart="true" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="SUBMIT"
                            android:textSize="18sp"
                            android:textStyle="bold"
                            android:textColor="@color/dark_yellow"
                            android:id="@+id/submitBtn"
                            android:layout_centerVertical="true"
                            android:layout_centerHorizontal="true" />

                        <RelativeLayout
                            android:id="@+id/submitDisableLayout"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_alignParentTop="true"
                            android:layout_alignParentLeft="true"
                            android:background="@drawable/disable_bg"
                            android:layout_alignParentStart="true">

                        </RelativeLayout>
                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="55dp"
                        android:layout_marginTop="15dp"
                        android:layout_marginBottom="15dp"
                        android:background="@drawable/white_bg"
                        android:layout_below="@+id/relativeLayout9"
                        android:layout_alignLeft="@+id/relativeLayout9"
                        android:layout_alignStart="@+id/relativeLayout9"
                        android:layout_alignRight="@+id/relativeLayout9"
                        android:layout_alignEnd="@+id/relativeLayout9"
                        android:id="@+id/relativeLayout10">

                        <ImageView
                            android:layout_width="25dp"
                            android:layout_height="25dp"
                            android:id="@+id/imageView10"
                            android:src="@drawable/inspect_button"
                            android:layout_marginLeft="5dp"
                            android:layout_centerVertical="true"
                            android:layout_alignParentLeft="true"
                            android:layout_alignParentStart="true" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="RECORD\nINSPECTION"
                            android:textSize="16sp"
                            android:textStyle="bold"
                            android:layout_marginLeft="14dp"
                            android:textColor="@color/dark_yellow"
                            android:id="@+id/inspectionBtn"
                            android:layout_centerVertical="true"
                            android:layout_toRightOf="@+id/imageView10"
                            android:layout_toEndOf="@+id/imageView10"
                            android:layout_marginStart="14dp" />

                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:id="@+id/resultLayout"
                        android:background="@drawable/white_bg"
                        android:layout_alignTop="@+id/relativeLayout10"
                        android:layout_alignLeft="@+id/relativeLayout11"
                        android:layout_alignStart="@+id/relativeLayout11"
                        android:layout_alignBottom="@+id/relativeLayout10"
                        android:layout_alignRight="@+id/relativeLayout11"
                        android:layout_alignEnd="@+id/relativeLayout11">


                        <ImageView
                            android:layout_width="25dp"
                            android:layout_height="25dp"
                            android:id="@+id/imageView11"
                            android:src="@drawable/result_button"
                            android:layout_marginLeft="5dp"
                            android:layout_centerVertical="true"
                            android:layout_alignParentLeft="true"
                            android:layout_alignParentStart="true" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="RESULT\nSHEET"
                            android:textSize="16sp"
                            android:textStyle="bold"
                            android:textColor="@color/dark_yellow"
                            android:id="@+id/resultBtn"
                            android:layout_centerVertical="true"
                            android:layout_centerHorizontal="true" />

                        <RelativeLayout
                            android:id="@+id/resultDisableLayout"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_alignParentTop="true"
                            android:background="@drawable/disable_bg"
                            android:layout_alignParentLeft="true"
                            android:layout_alignParentStart="true"></RelativeLayout>
                    </RelativeLayout>
                </RelativeLayout>
            </android.support.v7.widget.CardView>
            <android.support.v7.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_below="@+id/topLayout"
                android:id="@+id/middleCard"
                android:layout_marginTop="15dp">
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="?attr/actionBarSize"
                        android:background="@drawable/header_bg"
                        android:id="@+id/headerLayout"
                        android:layout_alignParentTop="true"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Basic Details"
                            android:textSize="22sp"
                            android:textColor="@color/white"
                            android:textStyle="bold"
                            android:layout_marginLeft="15dp"
                            android:id="@+id/text1"
                            android:layout_centerVertical="true"
                            android:layout_alignParentLeft="true"
                            android:layout_alignParentStart="true" />

                        <RelativeLayout
                            android:layout_width="110dp"
                            android:layout_margin="8dp"
                            android:background="@drawable/yellow_btn_bg"
                            android:layout_height="match_parent"
                            android:layout_centerVertical="true"
                            android:id="@+id/editlayout"
                            android:layout_alignParentRight="true"
                            android:layout_alignParentEnd="true">

                            <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="5dp"
                                android:id="@+id/imageView12"
                                android:src="@drawable/edit_pencil"
                                android:layout_centerVertical="true"
                                android:layout_alignParentLeft="true"
                                android:layout_alignParentStart="true" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="EDIT"
                                android:textStyle="bold"
                                android:layout_marginLeft="15dp"
                                android:textColor="@color/dark_yellow"
                                android:textSize="18sp"
                                android:id="@+id/text2"
                                android:layout_centerVertical="true"
                                android:layout_toRightOf="@+id/imageView12"
                                android:layout_toEndOf="@+id/imageView12" />
                        </RelativeLayout>
                    </RelativeLayout>

                </RelativeLayout>

            </android.support.v7.widget.CardView>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_alignParentLeft="true"
                android:layout_below="@+id/middleCard"
                android:layout_alignParentStart="true">


                <TextView
