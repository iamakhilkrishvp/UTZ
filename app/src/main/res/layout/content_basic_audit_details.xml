                    android:textSize="14sp"
                    android:layout_below="@+id/text5"
                    android:layout_alignLeft="@+id/text5"
                    android:layout_alignStart="@+id/text5"
                    android:layout_alignRight="@+id/farmNameText"
                    android:layout_alignEnd="@+id/farmNameText" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Year :"
                    android:textSize="16sp"
                    android:textColor="@color/colorPrimaryDark"
                    android:id="@+id/text6"
                    android:layout_below="@+id/auditorsText"
                    android:layout_alignLeft="@+id/auditorsText"
                    android:layout_alignStart="@+id/auditorsText"
                    android:layout_marginTop="15dp" />

                <EditText
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/yearText"
                    android:textSize="14sp"
                    android:layout_below="@+id/text6"
                    android:layout_alignLeft="@+id/text6"
                    android:layout_alignStart="@+id/text6"
                    android:layout_alignRight="@+id/auditorsText"
                    android:layout_alignEnd="@+id/auditorsText" />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Legal Status :"
                    android:textSize="16sp"
                    android:textColor="@color/colorPrimaryDark"
                    android:id="@+id/text7"
                    android:layout_below="@+id/yearText"
                    android:layout_alignLeft="@+id/yearText"
                    android:layout_alignStart="@+id/yearText"
                    android:layout_marginTop="15dp" />

                <EditText
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/auditTypeText"
                    android:layout_below="@+id/text7"
                    android:textSize="14sp"
                    android:layout_alignLeft="@+id/text7"
                    android:layout_alignStart="@+id/text7"
                    android:layout_alignRight="@+id/yearText"
                    android:layout_alignEnd="@+id/yearText" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Status :"
                    android:textSize="16sp"
                    android:textColor="@color/colorPrimaryDark"
                    android:id="@+id/text8"
                    android:layout_below="@+id/auditTypeText"
                    android:layout_alignLeft="@+id/auditTypeText"
                    android:layout_alignStart="@+id/auditTypeText"
                    android:layout_marginTop="15dp" />

                <EditText
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/statusText"
                    android:textSize="14sp"
                    android:layout_below="@+id/text8"
                    android:layout_alignLeft="@+id/text8"
                    android:layout_alignStart="@+id/text8"
                    android:layout_alignRight="@+id/auditTypeText"
                    android:layout_alignEnd="@+id/auditTypeText" />


                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Start Date - End Date :"
                    android:textSize="16sp"
                    android:textColor="@color/colorPrimaryDark"
                    android:id="@+id/text9"
                    android:layout_below="@+id/statusText"
                    android:layout_alignLeft="@+id/statusText"
                    android:layout_alignStart="@+id/statusText"
                    android:layout_marginTop="15dp" />

                <EditText
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/dateText"
                    android:textSize="14sp"
                    android:layout_below="@+id/text9"
                    android:layout_alignLeft="@+id/text9"
                    android:layout_alignStart="@+id/text9"
                    android:layout_alignRight="@+id/statusText"
                    android:layout_alignEnd="@+id/statusText" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Farm Details :"
                    android:textSize="20sp"
                    android:textColor="@color/colorPrimaryDark"
                    android:textStyle="bold"
                    android:id="@+id/text10"
                    android:layout_below="@+id/dateText"
                    android:layout_alignLeft="@+id/dateText"
                    android:layout_alignStart="@+id/dateText"
                    android:layout_marginTop="15dp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Total Area :"
                    android:textSize="16sp"
                    android:textColor="@color/colorPrimaryDark"
                    android:id="@+id/text11"
                    android:layout_below="@+id/text10"
                    android:layout_alignLeft="@+id/text10"
                    android:layout_alignStart="@+id/text10"
                    android:layout_marginTop="20dp" />

                <EditText
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/farmAreaText"
                    android:textSize="14sp"
                    android:layout_below="@+id/text11"
                    android:layout_alignLeft="@+id/text11"
                    android:layout_alignStart="@+id/text11"
                    android:layout_alignRight="@+id/dateText"
                    android:layout_alignEnd="@+id/dateText" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Address :"
                    android:textSize="16sp"
                    android:textColor="@color/colorPrimaryDark"
                    android:id="@+id/text12"
                    android:layout_below="@+id/farmAreaText"
                    android:layout_alignLeft="@+id/farmAreaText"
                    android:layout_alignStart="@+id/farmAreaText"
                    android:layout_marginTop="15dp" />

                <EditText
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/addressText"
                    android:textSize="14sp"
                    android:layout_below="@+id/text12"
                    android:layout_alignLeft="@+id/text12"
                    android:layout_alignStart="@+id/text12"
                    android:layout_alignRight="@+id/farmAreaText"
                    android:layout_alignEnd="@+id/farmAreaText" />

                <TextView
