                    android:layout_below="@+id/farmNameText"
                    android:layout_alignLeft="@+id/farmNameText"
                    android:layout_alignStart="@+id/farmNameText"
                    android:layout_marginTop="15dp" />

                <EditText
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/auditorsText"
